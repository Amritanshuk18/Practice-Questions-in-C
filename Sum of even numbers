#include<stdio.h>
int main()
{
    int i,a,b,sum=0;
    printf("Enter the upper and lower limit respectively : ");
    scanf("%d%d",&a,&b);
    if(b%2==0)
    {
        for(i=b;i<=a;i=i+2)
        {
            sum = sum+i;
        }
    }
    else
    {
        b=b+1;
        for(i=b;i<=a;i=i+2)
        {
            sum = sum+i;
        }
    }
    printf("Sum of all the even numbers between %d and %d = %d",b,a,sum);
}
