#include<stdio.h>
int main()
{
    int a,b;
    printf("Enter the two numbers : ");
    scanf("%d%d",&a,&b);
    if(a>b)
    {
        printf("Your num1 %d is greater",a);
    }
    else if(a<b)
    {
        printf("Your num2 %d is greater",b);
    }
    else
    {
        printf("Both num1 %d and num2 %d are equal",a,b);
    }
}
