#include<stdio.h>
int main()
{
    int r,c;
    for(r=1;r<=1;r++)
    {
        for(c=1;c<=5;c++)
        {
            printf("*");
        }
        printf("\n");
    }
    for(r=1;r<=3;r++)
    {
        for(c=1;c<=1;c++)
        {
            printf("*   *");
        }
        printf("\n");
    }
    for(r=1;r<=1;r++)
    {
        for(c=1;c<=5;c++)
        {
            printf("*");
        }
        printf("\n");
    }
    
    return 0;
}
